CyanChatSession
	private:
		gchar*				server
		guint				port
		gint				socket
		GHashTable*			buddies
		gchar*				nickname
		gchar*				reqnick
	
	virtual:
		name_accepted
		name_denied
		received_im
		received_chat
		add_chat_buddy
		remove_chat_buddy
		change_chat_buddy
		lobby_message
		ignore_chat_buddy
		unhandled_cmd
		get_version
	
	cyanchat_session_new(server, port)
	
	cyanchat_session_get_name(s) // "server:port"
	cyanchat_session_get_nickname(s)
	
	cyanchat_session_set_nickname(s, reqnick)

	cyanchat_session_connect(s)
	cyanchat_session_disconnect(s)
	cyanchat_session_send(s, const gchar* msg)

