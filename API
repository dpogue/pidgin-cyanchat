CyanChatBuddy
	private:
		gchar*				nickname
		CyanChatBuddyLevel	level
		gboolean			ignored
	
	cyanchat_buddy_new(nickname, level, ignored)
	cyanchat_buddy_new_from_username(username)
	cyanchat_buddy_get_nickname(b)
	cyanchat_buddy_get_level(b)
	cyanchat_buddy_get_ignored(b)

